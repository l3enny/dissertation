@article{Vasilyak1994,
abstract = {An analysis of experimental and theoretical investigations is the basis of this state-of-art review of models of fast ionisation waves (FIWs) and of characteristics and properties of these waves. The attention is concentrated on waves with the maximum possible velocities of 10 9 –2×10 10 cm s –1 when the amplitudes of voltage pulses are 20–300 kV. At low and moderate pressures the reduced intensity of the electric field in the front of a wave is so high that the front becomes a moving source of a beam of high-energy electrons in which the current can reach several kiloamperes. At moderate pressures the high-energy electrons in the wave front overtake the front and cause preliminary ionisation of the gas ahead of the front. At low pressures these electrons determine mainly the mechanism of the motion of the front. At high pressures (in excess of 200 torr) the main source of such preionisation is the radiation emitted by the front. The high rate of filling of the discharge volume with a plasma, high electric fields and high energies of the electrons in the front, and the slight heating of the gas make fast ionisation waves attractive for applications.},
annote = {Earlier review, with a strong focus on wave velocities. However, it also discusses the electric field in the front, and presents (the first?) occurence of the coaxial discharge geometry.
        
      },
author = {Vasilyak, L M and Kostyuchenko, S V and Kudryavtsev, N N and Filyugin, I V},
doi = {10.1070/PU1994v037n03ABEH000011},
file = {:home/ben/Documents/Papers/Vasilyak et al. - 1994 - Fast ionisation waves under electrical breakdown conditions.pdf:pdf},
issn = {1063-7869},
journal = {Physics-Uspekhi},
keywords = {experiment,npd,review,theory},
mendeley-tags = {experiment,npd,review,theory},
month = mar,
number = {3},
pages = {247--268},
title = {{Fast ionisation waves under electrical breakdown conditions}},
url = {http://stacks.iop.org/1063-7869/37/i=3/a=R02},
volume = {37},
year = {1994}
}
