issn = {0022-3727},
journal = {Journal of Physics D: Applied Physics},
month = aug,
number = {15},
pages = {4493--4502},
title = {{Plasma decay in N 2 , CO 2 and H 2 O excited by high-voltage nanosecond discharge}},
